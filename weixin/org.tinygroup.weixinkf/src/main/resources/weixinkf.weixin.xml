   </url-config>
   <url-config key="delKf"  method="get" >
     <url><![CDATA[https://api.weixin.qq.com/customservice/kfaccount/del?access_token=${ACCESS_TOKEN}&kf_account=${delKf.accountId}]]></url>
   </url-config>
   <url-config key="getKfList"  method="get" >
     <url><![CDATA[https://api.weixin.qq.com/cgi-bin/customservice/getkflist?access_token=${ACCESS_TOKEN}]]></url>
