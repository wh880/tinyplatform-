<url-configs>
   <url-config key="token"  method="get" >
     <url><![CDATA[https://api.weixin.qq.com/cgi-bin/token?grant_type=client_credential&appid=${token.appId}&secret=${token.secret}]]></url>
   </url-config>
   <url-config key="ticket"  method="get" >
     <url><![CDATA[https://api.weixin.qq.com/cgi-bin/ticket/getticket?access_token=${ticket.accessToken}&type=jsapi]]></url>
   </url-config>
   <url-config key="oauth_token"  method="get">
     <url><![CDATA[https://api.weixin.qq.com/sns/oauth2/access_token?appid=${WEIXIN_CLIENT.appId}&secret=${WEIXIN_CLIENT.secret}&code=${oauth_token.code}&grant_type=authorization_code]]></url>
   </url-config>
</url-configs>
