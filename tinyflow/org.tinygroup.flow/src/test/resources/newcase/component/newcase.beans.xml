<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd"
	   default-autowire="byType" default-lazy-init="true">

	<bean id="plusComponent" name="plusComponent" scope="prototype"
		class="org.tinygroup.flow.test.PlusComponent">
	</bean>
	<bean id="listComponent" name="listComponent" scope="prototype"
		class="org.tinygroup.flow.test.ListComponent">
	</bean>
	<bean id="sumComponent"  scope="prototype"
		class="org.tinygroup.flow.test.SumComponent">
	</bean>
	<bean id="imsubComponent" name="imsubComponent" scope="prototype"
		class="org.tinygroup.flow.test.ImsubComponent">
	</bean>
	<bean id="resetComponent" name="resetComponent" scope="prototype"
		class="org.tinygroup.flow.test.ResetComponent">
	</bean>
	<bean id="createExceptionComponent" name="createExceptionComponent" scope="prototype"
		class="org.tinygroup.flow.test.CreateExceptionComponent">
	</bean>
	<bean id="abcImsubComponent" name="abcImsubComponent" scope="prototype"
		class="org.tinygroup.flow.test.AbcImsubComponent">
	</bean>
	<bean id="abcPlusComponent" name="abcPlusComponent" scope="prototype"
		class="org.tinygroup.flow.test.AbcPlusComponent">
	</bean>
</beans>
