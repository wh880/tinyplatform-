<views>
	<view name="view1">
		<sqls>
                    <sql dialectTypeName="oracle"
                        content="CREATE OR REPLACE VIEW view1 AS  SELECT aaa.bb AS aa1,aaa.bb AS bb1 FROM aa aaa WHERE  aaa.bb=aaa.bb OR ( aaa.bb=null AND null='1' AND null='1' )  ORDER BY null ASC,aaa.bb DESC">
                    </sql>
        </sqls>
	</view>
	
</views>